{
  "name": "wedding-checkin-backend",
  "version": "1.0.0",
  "description": "Wedding guest check-in system backend",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "migrate": "node database/migrations/run.js",
    "backup": "node -e 'import(\"./util/databaseBackup.js\").then(m => m.default.createBackup())'",
    "restore": "node -e 'import(\"./util/databaseBackup.js\").then(m => m.default.restoreBackup(process.argv[1]))'",
    "create-superadmin": "node scripts/createSuperAdmin.js"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.0.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase-admin": "^11.10.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.0",
    "node-cron": "^3.0.2",
    "qrcode": "^1.5.3",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}